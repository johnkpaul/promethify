{
  "name": "promethify",
  "version": "0.1.0",
  "description": "Browserify plugin async load code like AMD",
  "main": "index.js",
  "scripts": {
    "test": "zuul-mp --local 3100 --reporter list -- test/test.js",
    "pretest": "npm run hint",
    "hint": "jshint --config .jshintrc ."
  },
  "promethify": {
    "basedir": "test",
    "port": 8089
  },
  "browserify": {
    "transform": [
      "./index.js",
      "requireify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/johnkpaul/promethify"
  },
  "keywords": [
    "browserify",
    "v3",
    "js",
    "plugin",
    "transform"
  ],
  "author": "John K. Paul",
  "license": "MIT",
  "dependencies": {
    "through": "~2.2.7",
    "innersource": "0.0.2",
    "detective": "~2.2.0",
    "inline-source-map": "~0.2.5",
    "combine-source-map": "~0.2.0",
    "lodash": "~2.2.1",
    "scriptjs": "~2.4.0",
    "requireify": "~0.1.1",
    "find-parent-dir": "~0.1.0",
    "q": "~0.9.7"
  },
  "devDependencies": {
    "browserify": "git://github.com/johnkpaul/node-browserify.git#add_basedir_for_globals",
    "jshint": "~2.1.11",
    "convert-source-map": "~0.2.6",
    "coffeeify": "~0.5.2",
    "zuul-mp": "~0.1.4"
  },
  "peerDependencies": {
    "browserify": ">=2.0.0",
    "scriptjs": ">=2.4.0"
  }
}
